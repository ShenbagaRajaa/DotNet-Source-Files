@model IEnumerable<BookDetailsShenba>;

<h2>Student Details</h2>
<p>
    <a asp-action="Create" asp-controller="Book">Add Student</a>
</p>
<table class="table">
    <tr>
            <td>Book Id</td>
            <td>Book Name </td>
            <td>Author</td>
            <td>Gereration</td>
            <td>Price</td>
            <td>Stock In Hand</td>
        </tr>
    @foreach(BookDetailsShenba s in Model){
        <tr>
            <td>@s.BookId</td>
            <td>@s.BookName </td>
            <td>@s.Author</td>
            <td>@s.Generation</td>
            <td>@s.Price</td>
            <td>@s.StockInHand</td>
            <td><a asp-action="Details" asp-controller="Book">Edit</a> | <a asp-action="Delete" asp-controller="Book" asp-route-id="@s.BookId">Delete</a></td>
        </tr>
    }
</table>